if __name__ == '__main__':
    n = int(raw_input())
    arr = map(int, raw_input().split())
    
    def secondLargest(arr) :
        largest = max(arr)
        secondLargest = min(arr)
        for x in arr :
            if secondLargest < x and x < largest:
                secondLargest = x
        return secondLargest   
    
    print(secondLargest(arr))
